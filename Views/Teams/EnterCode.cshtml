@{
    ViewData["Title"] = "Въведи код за отбор";
}

<h3>Въведи кода, който получи чрез SMS</h3>

<form asp-action="ValidateCode" method="post">
    <div class="form-group">
        <label for="code">Код:</label>
        <input type="text" name="code" class="form-control" />
    </div>
    <br />
    <button type="submit" class="btn btn-success">Провери</button>
</form>
